<div class="page-header">
	<h1>User list</h1>
</div>

<table class="table table-hover">
	<thead>
		<tr>
			<th>Name</th>
			<th>Login</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		{{#each users}}
			<tr>
				<td><a href="/users/{{login}}">{{name}}</a></td>
				<td>{{login}}</td>
				<td>{{email}}</td>
			</tr>
		{{/each}}
	</tbody>
</table>

